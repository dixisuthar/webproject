{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class
  ]
%}
<div{{ attributes.addClass(classes) }}>
  <div class="container">
    <div class="row">
    <h2>{{ date }}</h2>
    <h2>{{ city }}</h2>
      <h2>{{ country }}</h2>
      {% block content %}
        <div>
          {{ content }}
        </div>
      {% endblock %}
    </div>
  </div>
</div>
